{
    "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node", // Use 'node' resolution instead of nodenext
    "esModuleInterop": true,
      "declaration": true,
    "declarationDir": "dist",
      "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
  }
  